{
  "name": "nodejs-repo-template",
  "version": "1.0.0",
  "description": "This is a simple, yet complete, Node Repo Template. It can be used to easily and quickly create a project repo for your next NodeJS based project.",
  "main": "app.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "lint": "eslint './**/*.js' --quiet",
    "start": "node app",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "watch": "nodemon app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidsaulrodriguez/nodejs-repo-template.git"
  },
  "keywords": [
    "nodejs",
    "repo-template",
    "template",
    "starter-repo",
    "repo",
    "starter"
  ],
  "author": "David Saul Rodriguez II",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/davidsaulrodriguez/nodejs-repo-template/issues"
  },
  "homepage": "https://github.com/davidsaulrodriguez/nodejs-repo-template#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "nodemon": "^2.0.7"
  }
}
